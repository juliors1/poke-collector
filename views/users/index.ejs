<%- include('../partials/header') %>
<br>
<h3>Trainer List</h3>
<% users.forEach(u => { %>
  <a href="/users/<%= u._id %>">
    <div class="card">
      <div class="card-body">
        <img height="50" id="avatarPhoto" src="<%= u.avatar ? u.avatar : 'https://cdn.bulbagarden.net/upload/thumb/d/d3/Lets_Go_Pikachu_Eevee_Red.png/250px-Lets_Go_Pikachu_Eevee_Red.png' %>" alt="trainer logo">
        <%= u.name %></a> - Joined on <%= u.createdAt.toLocaleString().split(',')[0] %>
      </div>
    </div><br>
<% }) %>


<%- include('../partials/footer') %>